{
    "functions": {
      "api/index.py": {
        "runtime": "python3.10"
      }
    },
    "rewrites": [
      { "source": "/(.*)", "destination": "/api/index" }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "s-maxage=21600, stale-while-revalidate=59"
          }
        ]
      }
    ]
  }
