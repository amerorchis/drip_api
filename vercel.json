{
    "rewrites": [
      { "source": "/(.*)", "destination": "/api/index" }
    ],
    "headers": [
      {
        "source": "/api/index",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "s-maxage=21600, stale-while-revalidate=59"
          }
        ]
      }
    ]
  }
